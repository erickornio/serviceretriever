<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0.3//EN"
"http://google-web-toolkit.googlecode.com/svn/tags/2.5.1/distro-source/core/src/gwt-module.dtd">
<module rename-to='AdminPortal'>
	<inherits name='com.google.gwt.user.User' />
	<inherits name="com.google.gwt.resources.Resources" />
	<inherits name='com.google.gwt.user.theme.standard.Standard' />

	<inherits name='com.tractionsoftware.gwt.user.UTCDateTime' />

	<entry-point class='net.svcret.admin.client.AdminPortal' />

	<source path='client' />
	<source path='shared' />

	<collapse-all-properties />
	
	<!--
	Uncomment this during testing to only compile for Chrome/Safari 
	<set-property name="user.agent" value="safari" />  
	-->

</module>
